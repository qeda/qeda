{
  "name": "qeda",
  "version": "0.4.2",
  "description": "QEDA â€” a tool for creating libraries of electronic components",
  "main": "lib/qeda.js",
  "bin": {
    "qeda": "bin/qeda"
  },
  "scripts": {
    "prepublish": "coffee -o lib -c src/",
    "postpublish": "rm -rf lib",
    "premakedeb": "npm install",
    "makedeb": "node-deb -- bin/ lib/ share/ scripts/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qeda/qeda.git"
  },
  "node_deb": {
    "install_strategy": "copy",
    "init": "none",
    "entrypoints": {
      "cli": "bin/qeda"
    }
  },
  "keywords": [
    "EDA",
    "KiCad",
    "schematics",
    "PCB",
    "electronics",
    "library"
  ],
  "author": "QEDA Authors",
  "contributors": [
    "Matthias Breithaupt <m.breithaupt@vogl-electronic.com>",
    "Filippo Savi <filssavi@gmail.com>"
  ],
  "homepage": "http://qeda.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qeda/qeda/issues"
  },
  "devDependencies": {
    "coffeescript": "^2.7.0",
    "node-deb": "^0.10.7",
    "rm-r": "^1.0.2"
  },
  "dependencies": {
    "crc": "^4.3.2",
    "js-yaml": "^4.1.0",
    "nopt": "^7.2.1",
    "sprintf-js": "^1.1.3",
    "svg-path-parser": "^1.1.0",
    "sync-request": "^6.1.0"
  }
